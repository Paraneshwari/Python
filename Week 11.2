import math

def f1(num):
    try:
        n= float(num)
        if n< 0:
            print("Error: Cannot calculate the square root of a negative number.")
        else:
            result = math.sqrt(n)
            print("The square root of {} is {:.2f}".format(n, result))
    except ValueError:
        print("Error: could not convert string to float")


n= input()
f1(n)
