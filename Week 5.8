s=input()
start = 0
end = len(s) - 1
s = list(s)
while start < end:
    if not s[start].isalnum():
        start += 1
    elif not s[end].isalnum():
        end -= 1
    else:
        s[start], s[end] = s[end], s[start]
        start += 1
        end -= 1

print(''.join(s))

                
        
