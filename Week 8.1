n = int(input())
d = {}
for _ in range(n):
    a = input()
    s = ''
    l = []
    for i in a:
        if i.isalpha():
            s += i
        if i.isdigit():
            l.append(int(i))
    d[s] = l
for i in d:
    d[i] = sum(d[i])
min_total = min(d.values())
for name, total in d.items():
    if total == min_total:
        print(name, total)
for name, total in d.items():
    if total != min_total:
        print(name, total)
